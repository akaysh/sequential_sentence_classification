}, "text_id": "S1lmJUZFA7", "sid": 38}, {"labels": {"alignments": [8], "responsetype": "answer", "coarseresponse": "concur"}, "text_id": "S1lmJUZFA7", "sid": 39}, {"labels": {"alignments": [8], "responsetype": "other", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 40}, {"labels": {"alignments": [8], "responsetype": "other", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 41}, {"labels": {"alignments": [8], "responsetype": "other", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 42}, {"labels": {"alignments": [8], "responsetype": "other", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 43}, {"labels": {"alignments": [8], "responsetype": "other", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 44}, {"labels": {"alignments": [8], "responsetype": "other", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 45}, {"labels": {"alignments": [8], "responsetype": "other", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 46}, {"labels": {"alignments": [8], "responsetype": "other", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 47}, {"labels": {"alignments": [8], "responsetype": "other", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 48}, {"labels": {"alignments": [8], "responsetype": "structuring", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 49}, {"labels": {"alignments": [8], "responsetype": "structuring", "coarseresponse": "nonarg"}, "text_id": "S1lmJUZFA7", "sid": 50}], "metadata": {"anno": "anno13", "review": "SJg9GkF9nQ", "rebuttal": "S1lmJUZFA7", "conference": "ICLR2019", "title": "Selective Self-Training for semi-supervised Learning", "reviewer": "AnonReviewer1", "forum_id": "SyzrLjA5FQ", "rating": "5: Marginally below acceptance threshold", "confidence": "4: The reviewer is confident but not absolutely certain that the evaluation is correct"}}